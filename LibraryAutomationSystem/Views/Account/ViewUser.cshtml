@model LibraryAutomationSystem.Entity.User

@{
    ViewBag.Title = "ViewUser";
}

<h2>ViewUser</h2>

<table border="1">
   <tr>
       <td>Name</td>
       <td>UserName</td>
       <td>DOB</td>
       <td>DOJ</td>
       <td>Sex</td>
       <td>PhoneNumber</td>
       <td>Email</td>
       <td>Address</td>
   </tr>
   @foreach(LibraryAutomationSystem.Entity.User user  in(List<LibraryAutomationSystem.Entity.User>)ViewData["user"])
   {
       <tr>
           <td>
               @user.MemberName;
           </td>
           <td>
               @user.MemberUserName;
           </td>
           <td>
               @user.MemberDOB;
           </td>
           <td>
               @user.MemberDOJ;
           </td>
           <td>
               @user.MemberSex;
           </td>
           <td>
               @user.MemberPhoneNumber;
           </td>
           <td>
               @user.Email;
           </td>
           <td>
        
               @user.MemberAddress;
           </td>
           <td>
               @Html.ActionLink("Delete","Delete",new{ userName=user.MemberUserName})
           </td>
       </tr>
   }
</table>



