@model LibraryAutomationSystem.Entity.User

@{
    ViewBag.Title = "LoginUser";
}

<h2>LoginUser</h2>

<table>
    <tr>
        <td>
          @Html.Label("UserName")
        </td>
        <td>
            @Html.TextBoxFor(Model=>Model.memberUserName)

        </td>
    </tr>
    <tr>
        <td>
            @Html.Label("Password")
        </td>
        <td>
            @Html.TextBoxFor(Model=>Model.memberPassword)
        </td>
    </tr>
    <tr>
        <td colspan="2">
            <input type="button" value="Login" />
        </td>
    </tr>
</table>

